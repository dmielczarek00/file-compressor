      { "label": "Normalizacja", "name": "normalize", "type": "checkbox", "default": true}
    ]
  },
  "mp4": {
    "options":[
      { "label": "VideoBitrate", "name": "videoBitrate", "type": "select", "values": ["1000k", "900k", "800k"], "default": "1000k" },
      { "label": "AudioBitrate", "name": "audioBitrate", "type": "select", "values": ["128k", "192k", "320k"], "default": "192k" },
      { "label": "Rozdzielczość", "name": "resolution", "type": "select", "values": ["480p", "720p", "1080p"], "default": "720p" }
    ]
  }
